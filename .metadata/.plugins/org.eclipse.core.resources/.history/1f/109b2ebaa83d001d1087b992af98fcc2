package employes;

import java.util.ArrayList;
import java.util.GregorianCalendar;
import java.util.List;

public class Secretaire extends Employes {
	
	List<Object> listManager= new ArrayList<>();

	protected Secretaire(String leNom, String lePrenom, GregorianCalendar laDate, Adresse lAdresse) {
		super(leNom, lePrenom, laDate, lAdresse);
		// TODO Auto-generated constructor stub
	}
	public void ajoutSecretaire(Secretaire secretaire) {
		listManager.add(secretaire);
	}
	public void enleverSecretaire(Secretaire secretaire) {
		listManager.remove(secretaire);
	}

	
	
}
